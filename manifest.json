{
    "manifest_version": 2,
    "name": "Typefi T Notifier",
    "description": "This extension will open up Typefi Tea Request Google spreadsheet",
    "version": "1.0",
    "icons":
            {
                "16": "images/typefi16.png",
                "32": "images/typefi32.png",
                "64": "images/typefi64.png",
                "128": "images/typefi128.png"
            },
    "homepage_url": "https://goo.gl/aQqX2b",
    "background":
            {
                "scripts": ["js/background.js"],
                "persistent": true
            },
    "browser_action": {
        "default_icon": "images/typefi128.png",
        "default_popup": "popup.html",
        "default_title": "Typefi T Notifier"
    },
    "permissions": [
        "activeTab", "storage", "https://goo.gl/ghM5Md", "management",
        "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/css/bootstrap.min.css",
        "alarms", "notifications", "identity", "identity.email", "tts"
    ],
    "options_page": "options.html",
    "minimum_chrome_version": "36"
}
